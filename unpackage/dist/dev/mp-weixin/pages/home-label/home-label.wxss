page {
  background-color: #f5f5f5;
}
.label .label_box {
  background-color: #fff;
  margin-bottom: 10px;
}
.label .label_box .label-header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 14px;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #666;
  padding: 10px 15px;
  border-bottom: 1px #f5f5f5 solid;
}
.label .label_box .label-header .label-edit {
  color: #30b33a;
  font-weight: bold;
}
.label .label_box .label-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 15px 15px;
  padding-top: 0;
}
.label .label_box .label-content .label-content-item {
  position: relative;
  padding: 2px 5px;
  margin-top: 12px;
  margin-right: 10px;
  border: 1px #666 solid;
  font-size: 14px;
  color: #666;
  border-radius: 5px;
}
.label .label_box .label-content .label-content-item .icons-close {
  position: absolute;
  right: -8px;
  top: -8px;
  background-color: #fff;
  border-radius: 50%;
}
.no-data {
  text-align: center;
  padding: 50px 0;
  color: #999;
  font-size: 14px;
  width: 100%;
}

