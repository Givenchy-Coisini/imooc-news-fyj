.navbar .navbar-fixed {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  background-color: #f07373;
}
.navbar .navbar-fixed .navbar_context {
  height: 45px;
  padding: 0 15px;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.navbar .navbar-fixed .navbar_context .navbar-search {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  padding: 0 10px;
  height: 30px;
  border-radius: 30px;
  background-color: #fff;
}
.navbar .navbar-fixed .navbar_context .navbar-search .navbar-search_iocn {
  width: 10px;
  height: 10px;
  border: 1px red solid;
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar_context .navbar-search .navbar-search_text {
  font-size: 12px;
  color: #999;
}

